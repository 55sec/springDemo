<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="textEditor" class="com.xyw55.demo6.TextEditor">
        <property name="message" value="1"/>
    </bean>

    <bean id="cStartEventHandler" class="com.xyw55.demo6.CStartEventHandler"/>
    <bean id="cStopEventHandler" class="com.xyw55.demo6.CStopEventHandler"/>

    <bean id="customEventHandler" class="com.xyw55.demo6.CustomEventHandler"/>
    <bean id="customEventPublisher" class="com.xyw55.demo6.CustomEventPublisher"/>

</beans>